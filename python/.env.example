# Photobooth API Configuration
# Copy this file to .env and update with your actual values

# API endpoint for uploading images/GIFs
# Local development:
# API_UPLOAD_ENDPOINT=http://localhost:8000/api/images
# Docker:
# API_UPLOAD_ENDPOINT=http://localhost:8080/api/images
# Production:
API_UPLOAD_ENDPOINT=https://your-domain.com/api/images

# API authentication token (REQUIRED for uploads)
# Generate with: php artisan token:generate
# The token must have 'upload' ability
API_AUTH_TOKEN=your_api_token_here_from_token_generate_command

# Optional: Request timeout in seconds (default: 30)
API_TIMEOUT=30
